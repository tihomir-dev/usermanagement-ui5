{
  "sap.app": {
    "id": "com.customer.usermanagement.usermanagement.cards.users",
    "type": "card"
  },
  "sap.card": {
    "type": "Object",
    
    "actions": [
      {
        "type": "Custom",
        "parameters": {
        "action": "navigateToUsers"
    }
      }
    ],
    "data": {
      "json": {
        "total": 0,
        "activeCount": 0,
        "inactiveCount": 0
      }
    },
    "header": {
      "icon": {
        "src": "sap-icon://customer"
      },
      "title": "Users",
      "subtitle": "{/total} total users"
    },
    "content": {
      "groups": [
        {
          "title": "Statistics",
          "items": [
            {
              "label": "Inactive Users",
              "value": "{/inactiveCount}"
            }
          ]
        }
      ]
    }
  }
}