{
  "_version": "1.81.0",
  "sap.app": {
    "id": "com.customer.usermanagement.usermanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.1",
      "toolsId": "3606814d-3354-4090-bb07-cd89113c55ea"
    },
    "dataSources": {
      "userService": {
        "uri": "/api/users",
        "type": "JSON"
      },
      "groupService": {
        "uri": "/api/groups",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.integration": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.customer.usermanagement.usermanagement.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "com.customer.usermanagement.usermanagement.view",
        "controlId": "fcl",
        "controlAggregation": "beginColumnPages",
        "transition": "slide",
        "async": true
      },
      "routes": [
  {
    "name": "RouteMain",
    "pattern": "",
    "target": ["TargetMain"]
  },
  {
    "name": "RouteUsers",
    "pattern": "users",
    "target": ["TargetUsers"]
  },
  {
    "name": "userDetail",
    "pattern": "users/{userId}/{layout}",
    "target": ["userDetail"]  
  },
  {
    "name": "RouteGroups",
    "pattern": "groups",
    "target": ["TargetGroups"]
  }
],

      "targets": {
        "TargetMain": {
          "viewName": "Main",
          "viewId": "Main",
          "controlAggregation": "beginColumnPages"
        },
        "TargetUsers": {
          "viewName": "Users",
          "viewId": "Users",
          "controlAggregation": "beginColumnPages"
        },
        "userDetail": {
          "viewName": "DetailUsers",
          "viewId": "DetailUsers",
          "controlAggregation": "midColumnPages"
        },
        "TargetGroups": {
          "viewName": "Groups",
          "viewId": "Groups",
          "controlAggregation": "beginColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "com.customer.usermanagement.usermanagement.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comcustomerusermanagementusermanagement"
  }
}