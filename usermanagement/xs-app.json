{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/",
    "logoutMethod": "GET"
  },
  "routes": [
    {
      "source": "^/user-api/currentUser$",
      "target": "/currentUser",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/api/users/sync/?$",
      "target": "/users/sync",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/users/([^/]+)$",
      "target": "/users/$1",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/users(.*)$",
      "target": "/users$1",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/users/([^/]+)/groups$",
      "target": "/users/$1/groups",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/groups/sync/?$",
      "target": "/groups/sync",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/groups/([^/]+)/members/([^/]+)$",
      "target": "/groups/$1/members/$2",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/groups/([^/]+)/members(.*)$",
      "target": "/groups/$1/members$2",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/groups/([^/]+)$",
      "target": "/groups/$1",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/api/groups(.*)$",
      "target": "/groups$1",
      "destination": "apiservice-destination",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "xsuaa"
    }
  ]
}